{
  "manifest_version": 3,
  "name": "Chrome-Drill",
  "version": "1.1",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["output.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "icon.png"
  }
}
